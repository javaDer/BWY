.map_container {
  position: absolute;
  top: 0;
  bottom: 80px;
  left: 0;
  right: 0;
}

.myMap {
  width: 100%;
  height: 100%;
}

/*起始点输入框*/
.origin_input {
  margin-left: 1%;
  position: absolute;
  text-align: center;
  margin-bottom: 170rpx;
  left: 0;
  right: 0;
  bottom: 0px;
  background: #f6f6e0;
  padding: 0 25px;
  border-radius: 20px;
  width: 85%;
  height: 5%;
}
/*目的地输入框*/
.destination_input {
  position: absolute;
  margin-left: 1%;
  text-align: center;
  margin-bottom: 95rpx;
  left: 0;
  right: 0;
  bottom: 0px;
  background: #f6f6e0;
  padding: 0 25px;
  border-radius: 20px;
  width: 85%;
  height: 5%;
}
.destination_images{
   position: absolute;
   margin-left: -50%;
  }
input {
  margin: 5rpx 0;
  display: block;
  font-size: 0.4rpm;
  }
.primary_btn{
   position: absolute;
  text-align: center;
  margin-bottom: 9rpx;
  left: 0;
  right: 0;
  bottom: 0px;
  color: #f6f6e0;
  padding: 0 25px;
